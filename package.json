{
  "name": "bank-scraper",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/infra/http/server.js",
    "dev": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify src/infra/http/server.ts",
    "test": "jest",
    "check-types": "tsc --noemit",
    "lint:fix": "eslint src --ext ts --fix",
    "lint": "eslint src --ext ts && yarn check-types",
    "postinstall": "yarn build",
    "docs": "redoc-cli serve docs/swagger.yml --watch"
  },
  "dependencies": {
    "axios": "0.19.2",
    "celebrate": "12.1.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.24",
    "uuidv4": "6.1.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/hapi__joi": "17.1.4",
    "@types/jest": "26.0.4",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongoose": "5.7.31",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "axios-mock-adapter": "1.18.1",
    "eslint": "7.4.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.9.6"
  },
  "engines": {
    "node": "12.x"
  }
}
